@model IEnumerable<Frycz_pcdb.computer>

@{
    ViewBag.Title = "List of Computers";
}
<div class="container">
<h1 class="jumbotron-heading text-center">List of Computers</h1>
    <div>
        

        <button type="button" onclick="location.href = '@Url.Action("Index", "Home")'" class="btn btn-link">
            <span class="glyphicon glyphicon-backward"></span> Back to search
        </button>

    </div>
<table class="table">
    <tr>
        <th scope="col">
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th scope="col">
            @Html.DisplayNameFor(model => model.user.lastname)
        </th>
        <th scope="col">
            @Html.DisplayNameFor(model => model.user.firstname)
        </th>
        <th scope="col">
            @Html.DisplayNameFor(model => model.comment)
        </th>
        <th  scope="col">
            @Html.DisplayNameFor(model => model.computer_type.name)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <a href="@Url.Action("Index", "ComputerDetail", item)">@Html.DisplayFor(modelItem => item.name)   <span class="glyphicon glyphicon-edit"></span></a>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.lastname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.firstname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.comment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.computer_type.name)
        </td>
    </tr>
}

</table>
</div>